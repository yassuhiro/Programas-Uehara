#include <iostream>
#include <stdlib.h>
#include <typeinfo>

using namespace std;

int main() {
	int i,j;
	int A[3][3] = {{1,4,7},{2,5,8},{3,6,9}};
	int B[3][3] = {{1,5,1},{2,0,1},{3,1,1}};
	
	cout<<"Sua matriz A: \n";
	
	for(i=0;i<=2;i++){
		for(j=0;j<=2;j++){
			cout<<A[i][j]<<" ";
			}
	cout<<"\n";
	}
	
	cout<<"Sua matriz B: \n";
	
	for(i=0;i<=2;i++){
		for(j=0;j<=2;j++){
			cout<<B[i][j]<<" ";
			}
	cout<<"\n";
	}
	
	cout<<"\n\nA matriz A+B: \n";
	
	for(i=0;i<=2;i++){
		for(j=0;j<=2;j++){
			cout<<A[i][j]+B[i][j]<<" ";
			}
	cout<<"\n";
	}
	
	
	
	
	return 0;
	
	
	
}
